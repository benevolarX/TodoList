  "bugs": {
    "url": "https://github.com/benevolarX/TodoList/issues"
  },
  "homepage": "https://github.com/benevolarX/TodoList#readme",
  "dependencies": {
    "@benevolar/b-css": "0.0.3",
    "preact": "^8.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@types/node": "^10.12.19",
    "parcel-bundler": "^1.11.0",
    "rimraf": "^2.6.3",
    "typescript": "^3.2.4"
  }
